{
	"configurations": [
		{
			"name": "vscode-extension",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"type": "extensionHost",
			"outputCapture": "console",
			"runtimeExecutable": "${execPath}",
		}
	]
}